{
	"name": "kabooty",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev",
		"dev:host": "svelte-kit dev --host",
		"prod:start": "prisma migrate deploy && node server.js",
		"build": "npm run clean && svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"prepare": "svelte-kit sync",
		"clean": "rm -rf .svelte-kit && rm -rf build",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .prettierignore --check --plugin-search-dir=. . && eslint --ignore-path .prettierignore .",
		"format": "prettier --ignore-path .prettierignore --write --plugin-search-dir=. . && eslint --ignore-path .prettierignore . --fix",
		"prisma:generate": "prisma generate"
	},
	"pre-commit": [
		"lint"
	],
	"devDependencies": {
		"@playwright/test": "^1.21.0",
		"@sentry/types": "^7.6.0",
		"@sveltejs/adapter-node": "next",
		"@sveltejs/kit": "next",
		"@types/cookie": "^0.5.1",
		"@types/cron": "^2.0.0",
		"@types/events": "^3.0.0",
		"@types/jsonwebtoken": "^8.5.8",
		"@types/mime-types": "^2.1.1",
		"@types/node": "^17.0.35",
		"@types/sharp": "^0.30.4",
		"@types/webfontloader": "^1.6.34",
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"autoprefixer": "^10.4.4",
		"browserslist": "^4.20.2",
		"eslint": "^8.12.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"pre-commit": "^1.2.2",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.0",
		"prisma": "^3.15.2",
		"sass": "^1.49.11",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.10.5",
		"typescript": "~4.6.2"
	},
	"type": "module",
	"dependencies": {
		"@discordjs/builders": "^1.3.0",
		"@discordjs/rest": "^1.3.0",
		"@prisma/client": "^3.15.2",
		"@sentry/node": "^7.6.0",
		"@sentry/tracing": "^7.6.0",
		"axios": "^0.27.2",
		"cookie": "^0.5.0",
		"cron": "^2.1.0",
		"discord-api-types": "^0.37.14",
		"discord-oauth2": "^2.10.0",
		"discord.js": "^13.8.1",
		"dotenv": "^16.0.1",
		"image-type": "^4.1.0",
		"jsonwebtoken": "^8.5.1",
		"mime-types": "^2.1.35",
		"polka": "^0.5.2",
		"sharp": "^0.30.7",
		"svelte-easy-crop": "^1.0.7",
		"svelte-infinite-scroll": "^2.0.1",
		"svelte-intl-precompile": "^0.8.1",
		"svelte-simple-modal": "^1.3.3",
		"tslib": "^2.3.1",
		"webfontloader": "^1.6.28"
	}
}
